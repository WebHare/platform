  {
  }
>;


// -----------------------------------------------------------------------------
//
// XSDAnySimpleType
//

PUBLIC OBJECTTYPE XSDAnySimpleType EXTEND XSDSimpleType
< // ---------------------------------------------------------------------------
  //
  // Constructor
  //

  PUBLIC MACRO NEW(OBJECT xsdparser, STRING namespaceuri, STRING name, RECORD ARRAY facets)
  : XSDSimpleType(xsdparser, namespaceuri, name, DEFAULT OBJECT, "")
  {
    this->facets := facets;
  }
>;


