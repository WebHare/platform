  <form name="addressform" objectname="formtest.whlib#AddressForm" gid=".addressform" jshandler="anyformhandler">
    <page>
      <checkbox name="visiblegroup" title="" label="VisibleGroup" value="true" />
      <checkbox name="enablegroup" title="" label="EnableGroup" value="true" />
      <group name="addressgroup" title="Address group">
        <checkbox name="visiblefields" title="" label="VisibleFields" value="true" />
        <checkbox name="enablefields" title="" label="EnableFields" value="true" />
        <address name="address" countrylist="NL BE" options="nl-zip-force">
          <fieldtitle tag="nr_detail" title="Number and Addition" />
          <fieldtitle tag="zip" /><!-- Use tid derived from field tid and fieldtitle tag, i.e. "site.addressform.address-zip" -->
        </address>
        <textedit name="neighbourhood" />
        <richtext name="address2heading"><body xmlns="http://www.w3.org/1999/xhtml"><h2>Adddress 2</h2><p>countrylist=NL BE DE, options=nl-zip-suggest</p></body></richtext>
        <address name="address2" countrylist="NL BE DE" options="nl-zip-suggest">
          <fieldtitle tag="country" title="Address2" />
          <fieldtitle tag="nr_detail" title="Number and Addition2" />
        </address>
      </group>
      <group title="Address 3" name="address3group">
        <address name="address3" options="!nl-zip-suggest" />
      </group>
    </page>
  </form>

  <form name="redirectform" objectname="formtest.whlib#RedirectForm">
    <page>
      <richtext name="intro" title="">
        <body xmlns="http://www.w3.org/1999/xhtml">
          <p>This form will redirect in 10 seconds</p>
        </body>
      </richtext>
    </page>
    <page role="thankyou" redirect="about:blank">
    </page>
  </form>

  <form name="emailform" objectname="formtest.whlib#EmailForm">
    <page>
      <textedit validationchecks="email" name="email" title="Email" required="true" />
      <textedit validationchecks="email-maysendfrom" name="email_sendfrom" title="May send from"  />
    </page>
    <page role="thankyou">
      <richtext name="thankyoutext" title="">
        <body xmlns="http://www.w3.org/1999/xhtml">
          <h2>Done!</h2>
        </body>
      </richtext>
    </page>
  </form>

  <form name="workhandlingerrorsform" objectname="formtest.whlib#WorkHandlingErrorsForm">
    <page>
      <textedit name="mode" title="Mode" required="true" />
      <wt:twolevel name="twolevel" title="" />
    </page>
  </form>

  <form name="arrayform" objectname="formtest.whlib#ArrayForm" jshandler="arrayform">
    <page>
      <textedit name="text" title="A text"/>
      <array name="contacts" min="1" max="2" title="Contacts">
        <textedit name="name" title="Name"/>
        <upload name="photo" title="Photo" />
        <select name="gender" rowkeytype="integer" type="pulldown" title="Gender"/>
        <date name="wrd_dateofbirth" title="DOB" />
      </array>
      <array name="contacts2" min="0" max="1">
        <textedit name="name" />  <!-- shouldn't conflict with array above us -->
        <wt:tscustomcomp name="customcomp" title="customcomp" />
      </array>
      <array name="customarray" title="Custom components">
        <textedit name="name" title="Name"/>
        <wt:tscustomcomp name="customcomp" title="customcomp" />
        <wt:twolevel name="twolevel" title="twolevel" />
      </array>
    </page>
  </form>

  <form name="datetimeform" objectname="formtest.whlib#DateTimeTestForm" jshandler="anyformhandler">
    <page>
      <checkbox name="enable_fields" title="" label="Enable fields" value="true"/>
      <checkbox name="require_fields" title="" label="Require fields" value="true"/>
      <checkbox name="show_fields" title="" label="Show fields" value="true"/>
      <date name="dateofbirth" min="1900-02-03" max="now+1d" title="DateOfBirth" value="now" groupclasses="ut-dateofbirth" />
      <time name="time" title="SomeTime" />
      <time name="time_sec" title="SomeTime_sec" precision="seconds" />
      <select name="choice" type="checkbox" title="Choice">
        <option rowkey="date" title="a date">
          <date name="choice_date" min="1900-02-03" max="now+1d" title="" value="now" groupclasses="ut-dateofbirth" required="true" />
        </option>
        <option rowkey="time" title="a time">
          <time name="choice_time" title="" />
        </option>
      </select>
      <date name="weeknumbers" title="Weeknumbers" value="2019-07-08" />
    </page>
  </form>

  <form name="visibleconditionsform" objectname="formtest.whlib#VisibleConditionsTestForm" jshandler="anyformhandler">
    <page>
      <select name="first" title="First choice" type="radio">
        <option rowkey="a" title="A" />
        <option rowkey="b" title="B" />
      </select>
      <select name="second" title="Second choice" type="radio">
        <option rowkey="a_a" title="A A" />
        <option rowkey="a_b" title="A B" />
        <option rowkey="b_a" title="B A" />
        <option rowkey="b_b" title="B B" />
      </select>
      <select name="third" title="Third choice" type="radio">
        <option rowkey="a" title="A" />
        <option rowkey="b" title="B" />
      </select>
    </page>
  </form>

</formdefinitions>
