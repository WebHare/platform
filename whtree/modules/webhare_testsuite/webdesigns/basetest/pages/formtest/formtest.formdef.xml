  <form name="addressform" gid=".addressform" jshandler="anyformhandler" objectname="formtest.whlib#AddressForm">
    <page>
      <checkbox name="visiblegroup" label="VisibleGroup" title="" value="true" />
      <checkbox name="enablegroup" label="EnableGroup" title="" value="true" />
      <group name="addressgroup" title="Address group">
        <checkbox name="visiblefields" label="VisibleFields" title="" value="true" />
        <checkbox name="enablefields" label="EnableFields" title="" value="true" />
        <address name="address" countrylist="NL AF" options="nl-zip-force">
          <fieldtitle tag="nr_detail" title="Number and Addition" />
          <fieldtitle tag="zip" />  <!-- Use tid derived from field tid and fieldtitle tag, i.e. "site.addressform.address-zip" -->
        </address>
        <textedit name="neighbourhood" title="" />
        <richtext name="address2heading" title="">
          <body xmlns="http://www.w3.org/1999/xhtml"><h2>Address 2</h2><p>countrylist=NL BE DE, options=nl-zip-suggest</p></body>
        </richtext>
        <address name="address2" countrylist="NL BE DE - WORLD" options="nl-zip-suggest" title="">
          <fieldtitle tag="country" title="Address2" />
          <fieldtitle tag="nr_detail" title="Number and Addition2" />
        </address>
      </group>
      <group name="address3group" title="Address 3">
        <address name="address3" options="" title="" />
      </group>
      <group name="address4group" title="Address 4">
        <address name="address4" countrylist="NL" options="nl-zip-force" title="" />
      </group>
    </page>
  </form>

  <form name="redirectform" objectname="formtest.whlib#RedirectForm">
    <page>
      <richtext name="intro" title="">
        <body xmlns="http://www.w3.org/1999/xhtml">
          <p>This form will redirect in 10 seconds</p>
        </body>
      </richtext>
    </page>

    <page redirect="about:blank" role="thankyou" />
  </form>

  <form name="emailform" objectname="formtest.whlib#EmailForm">
    <page>
      <textedit name="email" required="true" title="Email" validationchecks="email" />
      <textedit name="email_sendfrom" title="May send from" validationchecks="email-maysendfrom" />
    </page>

    <page role="thankyou">
      <richtext name="thankyoutext" title="">
        <body xmlns="http://www.w3.org/1999/xhtml">
          <h2>Done!</h2>
        </body>
      </richtext>
    </page>
  </form>

  <form name="workhandlingerrorsform" objectname="formtest.whlib#WorkHandlingErrorsForm">
    <page>
      <textedit name="mode" required="true" title="Mode" />
      <wt:twolevel name="twolevel" title="" />
    </page>
  </form>

  <form name="arrayform" jshandler="arrayform" objectname="formtest.whlib#ArrayForm">
    <page>
      <textedit name="text" title="A text" />
      <array name="contacts" max="2" min="1" title="Contacts">
        <hidden name="row_uid" title="Row UID" />
        <textedit name="name" title="Name" />
        <upload name="photo" title="Photo" />
        <select name="gender" type="pulldown" rowkeytype="integer" title="Gender" />
        <date name="wrd_dateofbirth" title="DOB" />
        <checkbox name="confirm" label="Please confirm" title="" />
        <select name="favcolor" type="radio" title="Favorite color">
          <option rowkey="red" title="Red" />
          <option rowkey="yellow" title="Yellow" />
          <option rowkey="blue" title="Blue" />
        </select>
      </array>
      <array name="contacts2" max="1" min="0" title="More contacts">
        <textedit name="name" title="Name" />  <!-- shouldn't conflict with array above us -->
        <wt:tscustomcomp name="customcomp" title="customcomp" />
      </array>
      <array name="customarray" title="Custom components">
        <textedit name="name" title="Name" />
        <wt:tscustomcomp name="customcomp" title="customcomp" />
        <wt:twolevel name="twolevel" title="twolevel" />
      </array>
    </page>
  </form>

  <form name="datetimeform" jshandler="anyformhandler" objectname="formtest.whlib#DateTimeTestForm">
    <page>
      <checkbox name="enable_fields" label="Enable fields" title="" value="true" />
      <checkbox name="require_fields" label="Require fields" title="" value="true" />
      <checkbox name="show_fields" label="Show fields" title="" value="true" />
      <date name="dateofbirth" groupclasses="ut-dateofbirth" max="now+1d" min="1900-02-03" title="DateOfBirth" value="now" />
      <time name="time" title="SomeTime" />
      <time name="time_sec" precision="seconds" title="SomeTime_sec" />
      <select name="choice" type="checkbox" title="Choice">
        <option rowkey="date" title="a date">
          <date name="choice_date"
                groupclasses="ut-dateofbirth"
                max="now+1d"
                min="1900-02-03"
                required="true"
                title=""
                value="now" />
        </option>
        <option rowkey="time" title="a time">
          <time name="choice_time" title="" />
        </option>
      </select>
      <date name="weeknumbers" title="Weeknumbers" value="2019-07-08" />
    </page>
  </form>

  <form name="visibleconditionsform"
        classes="formdefclass1 formdefclass2"
        jshandler="anyformhandler"
        objectname="formtest.whlib#VisibleConditionsTestForm">
    <page>
      <select name="first" type="radio" title="First choice">
        <option rowkey="a" title="A" />
        <option rowkey="b" title="B" />
      </select>
      <select name="second" type="radio" title="Second choice">
        <option rowkey="a_a" title="A A" />
        <option rowkey="a_b" title="A B" />
        <option rowkey="b_a" title="B A" />
        <option rowkey="b_b" title="B B" />
      </select>
      <select name="third" type="radio" title="Third choice">
        <option rowkey="a" title="A" />
        <option rowkey="b" title="B" />
      </select>
    </page>
  </form>

</formdefinitions>
