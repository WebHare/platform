<formdefinitions xmlns="http://www.webhare.net/xmlns/publisher/forms"
                 xmlns:wt="http://www.webhare.net/xmlns/webhare_testsuite/testformcomponents"
                 gid="site">

  <form name="coretest" gid=".coretest" jshandler="coretest" objectname="formtest.whlib#CoreTestForm">
    <page>
      <textedit name="email" enabled="false" title="Email" validationchecks="email" />
      <textedit name="text" title="Text" />
      <textedit name="setvalidator" title="SetValidator" />
      <select name="radiotest" type="radio" required="1" rowkeytype="integer">
        <option rowkey="1" title="Option 1">
          <textedit name="radiotestnamelijk" required="true" tid=".namelijk" value="15" valuetype="integer" />
        </option>
        <option rowkey="2" selected="true" />
      </select>
      <select name="radioboolean" type="radio" rowkeytype="boolean" title="Radioboolean">
        <option rowkey="true" title="Truth" />
        <option rowkey="false" title="Lie" />
      </select>
      <checkbox name="showradioy" label="Show requiredradio Y" title="Radio" value="true" />
      <select name="requiredradio" type="radio" required="true" title="Required&amp;;Radio">
        <option rowkey="x" title="X" />
        <option rowkey="y" title="Y" />
        <option enabled="false" rowkey="z" title="Z" />
      </select>
      <select name="horizontalradio" type="radio" horizontal="true" title="Horizontal Radio">
        <option rowkey="x" title="X" />
        <option rowkey="y" title="Y" />
      </select>
      <select name="pulldowntest"
              type="pulldown"
              placeholdertid=".make_a_selection"
              required="true"
              rowkeytype="integer"
              title="Pulldowntest">
        <!-- <option rowkey="0" title="Make a selection" /> via placeholdertid -->
        <optgroup title="Below five">
          <option rowkey="1" title="One" />
          <option rowkey="2" title="Two" />
          <option enabled="false" rowkey="3" title="Three" />
        </optgroup>
        <optgroup title="Five and above">
          <option rowkey="5" title="Five" />
          <option rowkey="8" title="Eight" />
        </optgroup>
      </select>
      <select name="pulldown2test" type="pulldown" title="Pulldown2test">
        <option rowkey="red" title="Red" />
        <option rowkey="green" selected="true" title="Green">
          <textedit name="shade" title="Shade of green" />
        </option>
        <option rowkey="blue" title="Blue" />
      </select>
      <select name="pulldown3test" type="pulldown" placeholder="Select" title="Pulldown2test">
        <!-- placeholders broke earlier due to JS handler iisues -->
        <option rowkey="red" title="Red" />
        <option rowkey="green" title="Green">
          <textedit name="shade2" title="Shade of green" />
        </option>
        <option rowkey="blue" title="Blue" />
      </select>
      <select name="disabledpulldowntest" type="pulldown" enabled="false" tid=".disabledpulldown">
        <option rowkey="cant" title="Can't" />
        <option rowkey="touch" selected="true" title="Touch" />
        <option rowkey="this" title="This" />
      </select>
      <checkbox name="checkboxesvisible" title="Show checkboxes" value="true" />
      <select name="checkboxes" type="checkbox" max="2" min="1" rowkeytype="integer" title="Checkboxes">
        <option enabled="false" rowkey="0" title="Disabled" />
        <option rowkey="1" selected="true" title="Eins" />
        <option rowkey="2" tid=".zwei" />
        <option rowkey="3" selected="true" title="Polizei" />
      </select>
      <upload name="upload" tid="~upload" />
      <date name="dateofbirth"
            groupclasses="ut-dateofbirth"
            max="now+1d"
            min="1900-02-03"
            required="true"
            title="DateOfBirth"
            value="now" />
      <password name="password" minlength="2" placeholder="secret" title="Wachtwoord?" />
      <textedit name="number" max="2" min="-2" title="NumberPlease" valuetype="integer" />
      <textedit name="numberemptyvalue" emptyvalue="0" title="NumberEmpty" value="0" valuetype="integer" />
      <textedit name="nevervisible" title="I AM INVISIBLE" value="InvisValue" visible="false" />  <!-- static invisible -->
      <textedit name="invisible" title="I AM INVISIBLE" value="InvisValue" />  <!-- dynamic invisible -->
      <address name="address" countrylist="NL BE CA" title="Address" />
      <wt:twolevel name="twolevel" groupclasses="wh-testsuite-twolevel-groupclasses" title="" />
      <textarea name="textarea" minlength="3" maxlength="70" placeholder="Type a text in this area" title="Textarea" />
      <checkbox name="agree" labeltid=".terms" required="true" title="Agree?" />
      <checkbox name="condition_or_1" title="OR 1" />
      <checkbox name="condition_or_2" title="OR 2" />
      <textedit name="condition_or_visible" title="OR visible" />
      <checkbox name="condition_and_1" title="AND 1" />
      <checkbox name="condition_and_2" title="AND 2" />
      <textedit name="condition_and_visible" title="AND visible" />
      <checkbox name="condition_not" title="NOT" />
      <textedit name="condition_not_enabled" title="NOT enabled" />
      <textedit name="condition_not_required" title="NOT required" />
      <select name="condition_options" type="pulldown" rowkeytype="integer" title="Conditional options">
        <option rowkey="1" title="Eins selected" />
        <option rowkey="2" tid=".zwei" />
        <option rowkey="3" title="Polizei" />
      </select>
      <wt:matchattributes name="matchattributes_type1" attr1="type1" title="type1" />
      <wt:matchattributes name="matchattributes_type2_true" attr1="type2" attr2="true" title="type2_true" />
      <wt:matchattributes name="matchattributes_type2_false" attr1="type2" attr2="false" title="type2_false" />
      <group name="richtextgroup" title="">
        <!-- TODO why is there a 'richtextgroup' ? -->
        <richtext name="richtexttid" title="">
          <body xmlns="http://www.w3.org/1999/xhtml">
            <h2>before <span data-wh-gettid=".richtext-tid">This text is replaced by the 'site.coretest.richtext-tid' tid value</span> after</h2>
          </body>
        </richtext>
      </group>
      <hidden name="hidden" title="" value="value-xml" />
    </page>
  </form>

  <form name="dynamictest" jshandler="dynamicform" objectname="formtest.whlib#DynamicTestForm">
    <page>
      <select name="day" type="pulldown" required="true" rowkeytype="integer" title="Day">
        <option rowkey="1" title="Monday" />
        <option rowkey="2" title="Tuesday" />
      </select>
      <select name="timeslot" type="radio" required="true" rowkeytype="datetime" title="Timeslot">
        <option rowkey="2012-05-05T17:00:00" title="5 o'clock" />
        <option rowkey="2012-05-05T21:00:00.005" title="9 o'clock and 5 ms" />
      </select>
    </page>
  </form>

  <form name="rtdtest" jshandler="rtdform" objectname="formtest.whlib#RTDForm">
    <page>
      <checkbox name="enablefields" label="EnableFields" title="" value="true" />
      <checkbox name="requirefields" label="RequireFields" title="" value="false" />
      <rtd name="rtd" rtdtype="http://www.webhare.net/xmlns/webhare_testsuite/rtd/level1" title="RTD" />
      <imgedit name="img" title="Image" />
      <upload name="file" title="File" />
      <checkbox name="showfile2" title="Show file #2" />
      <upload name="file2" required="true" title="File #2" />
    </page>
  </form>

