<formdefinitions xmlns="http://www.webhare.net/xmlns/publisher/forms" xmlns:wt="http://www.webhare.net/xmlns/webhare_testsuite/testformcomponents" gid="site">
  <form name="coretest" gid='.coretest' objectname="formtest.whlib#CoreTestForm" jshandler="coretest">
    <page>
      <textedit validationchecks="email" name="email" title="Email" enabled="false" />
      <textedit name="text" title="Text" />
      <textedit name="setvalidator" title="SetValidator" />
      <select name="radiotest" rowkeytype="integer" type="radio" required="1">
        <option rowkey="1" title="Option 1">
          <textedit name="radiotestnamelijk" value="15" tid=".namelijk" valuetype="integer" required="true" />
        </option>
        <option rowkey="2" selected="true" />
      </select>
      <select name="radioboolean" rowkeytype="boolean" title="Radioboolean" type="radio">
        <option rowkey="true" title="Truth" />
        <option rowkey="false" title="Lie" />
      </select>
      <checkbox name="showradioy" title="Radio" label="Show requiredradio Y" value="true"/>
      <select name="requiredradio" title="Required&amp;;Radio" type="radio" required="true">
        <option rowkey="x" title="X" />
        <option rowkey="y" title="Y" />
        <option rowkey="z" title="Z" enabled="false"/>
      </select>
      <select name="horizontalradio" title="Horizontal Radio" type="radio" horizontal="true">
        <option rowkey="x" title="X" />
        <option rowkey="y" title="Y" />
      </select>
      <select name="pulldowntest" rowkeytype="integer" placeholdertid=".make_a_selection" title="Pulldowntest" type="pulldown" required="true">
        <!-- <option rowkey="0" title="Make a selection" /> via placeholdertid -->
        <optgroup title="Below five">
          <option rowkey="1" title="One" />
          <option rowkey="2" title="Two" />
          <option rowkey="3" title="Three" enabled="false" />
        </optgroup>
        <optgroup title="Five and above">
          <option rowkey="5" title="Five" />
          <option rowkey="8" title="Eight" />
        </optgroup>
      </select>
      <select name="pulldown2test" title="Pulldown2test" type="pulldown">
        <option rowkey="red" title="Red" />
        <option rowkey="green" title="Green" selected="true">
          <textedit name="shade" title="Shade of green" />
        </option>
        <option rowkey="blue" title="Blue" />
      </select>
      <select name="pulldown3test" title="Pulldown2test" type="pulldown" placeholder="Select"> <!-- placeholders broke earlier due to JS handler iisues -->
        <option rowkey="red" title="Red" />
        <option rowkey="green" title="Green">
          <textedit name="shade2" title="Shade of green" />
        </option>
        <option rowkey="blue" title="Blue" />
      </select>
      <select name="disabledpulldowntest" tid=".disabledpulldown" type="pulldown" enabled="false">
        <option rowkey="cant" title="Can't" />
        <option rowkey="touch" title="Touch" selected="true" />
        <option rowkey="this" title="This" />
      </select>
      <checkbox name="checkboxesvisible" title="Show checkboxes" value="true" />
      <select name="checkboxes" rowkeytype="integer" title="Checkboxes" type="checkbox" min="1" max="2">
        <option rowkey="0" title="Disabled" enabled="false" />
        <option rowkey="1" title="Eins" selected="true" />
        <option rowkey="2" tid=".zwei" />
        <option rowkey="3" title="Polizei" selected="true"/>
      </select>
      <upload name="upload" tid="tollium:common.actions.upload" />
      <date name="dateofbirth" min="1900-02-03" max="now+1d" title="DateOfBirth" value="now" groupclasses="ut-dateofbirth" required="true" />
      <password name="password" title="Wachtwoord?" placeholder="secret" />
      <textedit name="number" title="NumberPlease" valuetype="integer" min="-2" max="2"/>
      <textedit name="numberemptyvalue" title="NumberEmpty" valuetype="integer" emptyvalue="0" value="0" />
      <textedit name="nevervisible" title="I AM INVISIBLE" value="InvisValue" visible="false"/><!-- static invisible -->
      <textedit name="invisible" title="I AM INVISIBLE" value="InvisValue"/><!-- dynamic invisible -->
      <address name="address" title="Address" countrylist="NL BE" />
      <wt:twolevel name="twolevel" title="" groupclasses="wh-testsuite-twolevel-groupclasses"/>
      <textarea name="textarea" title="Textarea" placeholder="Type a text in this area" maxlength="70" />
      <checkbox name="agree" title="Agree?" required="true" labeltid=".terms"/>

      <checkbox name="condition_or_1" title="OR 1" />
      <checkbox name="condition_or_2" title="OR 2" />
      <textedit name="condition_or_visible" title="OR visible" />
      <checkbox name="condition_and_1" title="AND 1" />
      <checkbox name="condition_and_2" title="AND 2" />
      <textedit name="condition_and_visible" title="AND visible" />
      <checkbox name="condition_not" title="NOT" />
      <textedit name="condition_not_enabled" title="NOT enabled" />
      <textedit name="condition_not_required" title="NOT required" />

      <select name="condition_options" title="Conditional options" type="pulldown" rowkeytype="integer">
        <option rowkey="1" title="Eins selected" />
        <option rowkey="2" tid=".zwei" />
        <option rowkey="3" title="Polizei" />
      </select>

      <wt:matchattributes name="matchattributes_type1" title="type1" attr1="type1" />
      <wt:matchattributes name="matchattributes_type2_true" title="type2_true" attr1="type2" attr2="true" />
      <wt:matchattributes name="matchattributes_type2_false" title="type2_false" attr1="type2" attr2="false" />

      <group name="richtextgroup" title="">
        <richtext name="richtexttid" title="">
          <body xmlns="http://www.w3.org/1999/xhtml">
            <h2>before <span data-wh-gettid=".richtext-tid">This text is replaced by the 'site.coretest.richtext-tid' tid value</span> after</h2>
          </body>
        </richtext>
      </group>
    </page>
  </form>

  <form name="dynamictest" objectname="formtest.whlib#DynamicTestForm">
    <select name="day" rowkeytype="integer" title="Day" type="pulldown" required="true">
      <option rowkey="1" title="Monday" />
      <option rowkey="2" title="Tuesday" />
    </select>
    <select name="timeslot" rowkeytype="datetime" title="Timeslot" type="radio" required="true">
      <option rowkey="2012-05-05T17:00:00" title="5 o'clock" />
      <option rowkey="2012-05-05T21:00:00.005" title="9 o'clock and 5 ms" />
    </select>
  </form>

  <form name="rtdtest" objectname="formtest.whlib#RTDForm" jshandler="rtdform">
    <page>
      <checkbox name="enablefields" title="" label="EnableFields" value="true" />
      <checkbox name="requirefields" title="" label="RequireFields" value="false" />
      <rtd name="rtd" rtdtype="http://www.webhare.net/xmlns/webhare_testsuite/rtd/level1" title="RTD" />
      <imgedit name="img" title="Image" />
      <upload name="file" title="File" />
      <checkbox name="showfile2" title="Show file #2" />
      <upload name="file2" title="File #2" required="true" />
    </page>
  </form>

