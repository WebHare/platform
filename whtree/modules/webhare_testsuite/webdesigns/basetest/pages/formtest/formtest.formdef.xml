      <group name="hiddengroup" title="">
        <textedit name="hidden" title="Cannot fill this in" />
      </group>
      <select name="fillextra" type="checkbox" rowkeytype="integer" title="">
        <option rowkey="1" title="Fill an extra field" />
      </select>
      <textedit name="extra" required="true" title="Fill an extra field?" />
      <select name="fillother" type="checkbox" title="">
        <option rowkey="yep" title="Fill another field?">
          <textedit name="other" required="true" title="" />
        </option>
      </select>
      <select name="bonusquestion" type="radio" title="What...is the airspeed velocity of an unladen swallow?">
        <option rowkey="answer1" title="16 km/h" />
        <option rowkey="answer2" title="10 mph" />
        <option rowkey="answer3" title="What do you mean, an African or a European swallow?" />
      </select>
      <imgedit name="img" title="Image" />
