<?xml version="1.0" encoding="UTF-8"?>
<siteprofile xmlns="http://www.webhare.net/xmlns/publisher/siteprofile"
             xmlns:p="http://www.webhare.net/xmlns/publisher/components"
             xmlns:t="http://www.webhare.net/xmlns/tollium/screens"
             isscoped="true"
             >
  <contenttype namespace="scope:mediafolder">
    <member name="textfield" type="string" />
  </contenttype>
  <foldertype typedef="scope:mediafolder" title="Mediafolder (webhare_testsuite packagetest)" />
  <tabsextension name="mediafolder" xmlns="http://www.webhare.net/xmlns/tollium/screens">
    <newtab title="Mediafolder">
      <textedit composition="contentdata" cellname="textfield" title="MediafolderTextfield" />
    </newtab>
  </tabsextension>
  <apply>
    <to type="folder" foldertype="scope:mediafolder" />
    <extendproperties extension="#mediafolder" contenttype="scope:mediafolder" />
  </apply>
</siteprofile>
