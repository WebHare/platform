<?xml version="1.0" encoding="UTF-8"?>
<siteprofile xmlns="http://www.webhare.net/xmlns/publisher/siteprofile"
             xmlns:p="http://www.webhare.net/xmlns/publisher/components"
             gid="testtids.gid1">

  <tabsextension xmlns="http://www.webhare.net/xmlns/tollium/screens" name="educationfolderfolder">
    <newtab gid=".gid2" tid=".title">
      <textedit composition="contentdata" cellname="celletje" height="1pr" tid=".subtitle" />
    </newtab>
  </tabsextension>

  <tabsextension xmlns="http://www.webhare.net/xmlns/tollium/screens" name="multiplecompositions">
    <compositions>
      <p:whfsinstance name="other" typedef="http://www.utwente.nl/webhare/ws2013/otherdata" />
    </compositions>
    <newtab gid=".gid2" tid=".title">
      <textedit composition="contentdata" cellname="celletje" height="1pr" tid=".subtitle" />
    </newtab>
    <newtab title="Tab2">
      <textedit composition="other" cellname="celletje" height="1pr" tid=".gid2.subtitle" />
    </newtab>
  </tabsextension>

  <apply>
    <to type="folder" foldertype="http://www.utwente.nl/webhare/ws2013/educationfolderfolder" />
    <extendproperties extension="#educationfolderfolder"
                      contenttype="http://www.utwente.nl/webhare/ws2013/educationfolderfolder" />
    <extendproperties extension="#multiplecompositions"
                      contenttype="http://www.utwente.nl/webhare/ws2013/educationfolderfolder" />
  </apply>

</siteprofile>
