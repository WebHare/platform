<screens xmlns="http://www.webhare.net/xmlns/tollium/screens"
         xmlns:socialite="http://www.webhare.net/xmlns/socialite/components"
         library="iconviewer.whlib"
         gid="tolliumapps.iconviewer"
         >

  <messageboxes>
  </messageboxes>

  <screen name="iconviewer"
          title="Icon viewer"
          >

    <actions>
      <action name="refresh" onexecute="dorefresh" tid="~refresh" />
    </actions>

    <menus>
    </menus>

    <compositions>
      <eventlistener name="eventlistener" onevents="gotfilechange" />
    </compositions>

    <body layout="left" defaultbutton="btn_refresh">
      <textedit name="iconname" onchange="GotIconSelection">
        <autosuggest suggestobjectname="mod::webhare_testsuite/tolliumapps/iconviewer/iconviewer.whlib#AutocompleteIcon" />
      </textedit>
      <text name="displayed" />
      <text name="counter" visible="true" />
      <grid>
        <col width="10px" />
        <col width="40px" />
        <col width="10px" />
        <col width="48px" />
        <row>
          <cell><text tid=".b16" /></cell>
          <cell><image name="b16" width="16px" height="16px" title="" /></cell>
          <cell><text tid=".b24" /></cell>
          <cell><image name="b24" width="24px" height="24px" title="" /></cell>
        </row>
        <row>
          <cell><text tid=".b16" /></cell>
          <cell backgroundcolor="#95cdfe"><image name="bs16" width="16px" height="16px" title="" /></cell>
          <cell><text tid=".b24" /></cell>
          <cell backgroundcolor="#95cdfe"><image name="bs24" width="24px" height="24px" title="" /></cell>
        </row>
        <row>
          <cell><text tid=".c16" /></cell>
          <cell><image name="c16" width="16px" height="16px" title="" /></cell>
          <cell><text tid=".c24" /></cell>
          <cell><image name="c24" width="24px" height="24px" title="" /></cell>
        </row>
        <row>
          <cell><text tid=".c16" /></cell>
          <cell backgroundcolor="#95cdfe"><image name="cs16" width="16px" height="16px" title="" /></cell>
          <cell><text tid=".c24" /></cell>
          <cell backgroundcolor="#95cdfe"><image name="cs24" width="24px" height="24px" title="" /></cell>
        </row>
         <row>
          <cell><text tid=".w16" /></cell>
          <cell backgroundcolor="#4a4a4a"><image name="w16" width="16px" height="16px" title="" /></cell>
          <cell><text tid=".w24" /></cell>
          <cell backgroundcolor="#4a4a4a"><image name="w24" width="24px" height="24px" title="" /></cell>
        </row>
      </grid>
    </body>
    <footer layout="right">
      <button name="btn_refresh" action="refresh" tid="~refresh" />
    </footer>
  </screen>

</screens>
