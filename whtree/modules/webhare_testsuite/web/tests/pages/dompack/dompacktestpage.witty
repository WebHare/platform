  <button id="scrollable" type="button">scrollable</button>
  <button id="toggleclass" type="button">toggleclass</button>
  <div id="topmiddle">
    <select class="scrollable" id="alwaysopendown">
      <option selected disabled value="" class="copyclass" data-wh-form-placeholder>Should always open down</option>
      <option value="1" class="copy2">One</option>
      <option id="togglethisclass" value="2">Two</option>
      <option value="3">Third item that should always be opening down</option>
    </select>
    <select class="scrollable" id="ridiculous">
      <option selected disabled value="">Should also open down</option>
      <option value="1">D_One</option>
      <option value="2">D_Two</option>
      <option disabled value="3">D_Three</option>
    </select>
  </div>
  <form id="onepixelshort">
