[component merge]
  <p id="mergetest1">Come on <span data-merge="textContent:a.b">not-set</span>, just one more page!</p>
  <p id="mergetest2">Test my formula <span data-merge="textContent:makeupper(a.b)">not-set</span></p>
  <p id="mergetest3">Link <a href="not-set" data-merge="href:a.l;textContent:makeupper(a.b)">not-set</a></p>
  <p id="mergetest4">Link <span data-merge="upd(*)">not-set</a></p>
  <p id="mergetest5">Link <span data-merge="upd(subdata)">not-set</a></p>
  <p id="mergetest6">Link <span data-merge="textContent:selectsubtext(*)">not-set</a></p>
[/component]

