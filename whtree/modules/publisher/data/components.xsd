          <tai:tolliumcomponent fragment="/tolliumcomponents/conditionedit.xml#conditionedit" placement="block" />
          <tai:extraparser type="s:array" field="types" target="./p:conditiontype" />  <!-- reference to namespace MUST be explicit, otherwise xpath will be evaluating using the xmlns of the target node -->
        </xs:appinfo>
      </xs:annotation>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="conditiontype" />
      </xs:sequence>
      <xs:attributeGroup ref="tc:ComposableComponentBase" />
      <xs:attributeGroup ref="sc:Required" />
