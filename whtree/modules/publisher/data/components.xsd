          </xs:appinfo>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>

    <!-- Type names must be unique -->
    <xs:unique name="typenames">
      <xs:selector xpath="./p:conditiontype" />
      <xs:field xpath="@name" />
    </xs:unique>
  </xs:element>

