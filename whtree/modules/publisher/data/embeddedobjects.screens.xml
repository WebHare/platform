<siteprofile xmlns="http://www.webhare.net/xmlns/publisher/siteprofile">

  <tabsextension xmlns="http://www.webhare.net/xmlns/tollium/screens"
                 name="embedhtml"
                 allowresize="true"
                 gid="widgets.htmlwidget"
                 implementation="lib"
                 lib="components/embedhtml.whlib">
    <newtab>
      <textarea name="html"
                width="1pr"
                height="1pr"
                minwidth="70x"
                minheight="10x"
                maxlength="4096"
                maxlengthmeasure="bytes"
                showcounter="true" />
      <textarea composition="contentdata" cellname="html" name="actualhtml" visible="false" />
    </newtab>
  </tabsextension>

</siteprofile>
