<siteprofile
   xmlns="http://www.webhare.net/xmlns/publisher/siteprofile"
   xmlns:p="http://www.webhare.net/xmlns/publisher/components"
   xmlns:t="http://www.webhare.net/xmlns/tollium/screens">
  <!-- Reorganized and hopefully final content types -->
  <contenttype namespace="http://www.webhare.net/xmlns/publisher/designfolder">
    <member name="repositories" type="array">
      <member name="server" type="string" />
      <member name="repository" type="string" />
      <member name="uuid" type="string" />
      <member name="version" type="integer" />
    </member>
  </contenttype>

  <contenttype namespace="http://www.webhare.net/xmlns/publisher/localdesignsettings" cloneoncopy="false">
    <member name="outputmode_js" type="integer" />
    <member name="outputmode_css" type="integer" />
    <member name="repositories" type="array">
      <member name="localversion" type="whfsref" />
      <member name="uuid" type="string" />
    </member>
  </contenttype>

  <contenttype namespace="http://www.webhare.net/xmlns/publisher/designfiles/cachedfolder" cloneoncopy="true">
    <member name="hash" type="string" />
  </contenttype>

  <foldertype
    typedef="http://www.webhare.net/xmlns/publisher/designfolder"
    tid="foldertypes.designfolder"
    tolliumicon="tollium:folders/system"
    indexfile="none"
    isdevelopertype="true"
    ishidden="true"
    />
</siteprofile>
