<siteprofile xmlns="http://www.webhare.net/xmlns/publisher/siteprofile">

  <apply>
    <to type="all" />
    <webdesign assetpack="publisher:__dummy__"
               designfolder="."
               objectname="nowebdesign.whlib#NoWebdesign"
               wittyencoding="HTML-NI" />
    <disablelegacysitesettings notemplateprofile="true" />
    <baseproperties keywords="false" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/msworddownload" />
    <uploadtypemapping filetype="http://www.webhare.net/xmlns/publisher/msworddownload" mimetypemask="application/msword" />
    <uploadtypemapping filetype="http://www.webhare.net/xmlns/publisher/msworddownload"
                       mimetypemask="application/vnd.openxmlformats-officedocument.wordprocessingml.document" />
    <denyfoldertype typemask="*" />
    <denyfiletype typemask="*" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/unknownfile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/htmlfile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/javascriptfile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/imagefile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/pdffile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/cssfile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/msworddownload" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/externallink" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/internallink" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/contentlink" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/plaintextfile" />
    <allowfiletype typemask="http://www.webhare.net/xmlns/publisher/xmlfile" />
    <allowfoldertype typemask="http://www.webhare.net/xmlns/publisher/normalfolder" />
    <allowfoldertype typemask="http://www.webhare.net/xmlns/publisher/systemfolder" />
    <allowfoldertype typemask="http://www.webhare.net/xmlns/publisher/foreignfolder" />
  </apply>

</siteprofile>
