[component previewexpired]
  <p class="normal">
    This preview is no longer available
  </p>
[/component]

[component previewpassword]
  <p class="normal">
    This preview requires a password
  </p>
  <form action="[feedbackurl]" method="post">
    [if incorrectpassword]Incorrect password![/if]<br/>
    Password: <input name="password" type="password" required /><br/>
    <input type="submit" name="View page" />
  </form>
[/component]
