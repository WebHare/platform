<?wh

LOADLIB "mod::publisher/lib/webdesign.whlib";

LOADLIB "mod::system/lib/resources.whlib";


PUBLIC OBJECTTYPE CaptchaIntegrationPlugin EXTEND WebDesignPluginBase <
  PUBLIC STRING skipfunction;

  UPDATE PUBLIC MACRO ConfigureYamlPlugin(OBJECT webcontext, RECORD ARRAY props) {
    FOREVERY(RECORD p FROM props)
      IF(CellExists(p, "skip_function"))
        this->skipfunction := MakeAbsoluteResourcePath(p.source.siteprofile, p.skip_function);
  }
>;
