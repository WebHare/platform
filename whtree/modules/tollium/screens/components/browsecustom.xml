<screens xmlns="http://www.webhare.net/xmlns/tollium/screens"
         gid="components.browsecustom"
         library="/lib/components/browsecustom.whlib">

  <fragment name="browsecustom" gid=".browsecustom">
    <actions>
      <action name="browse" onexecute="dobrowse" title="" />
      <action name="clear" onexecute="doclear" title="" />
    </actions>
    <contents>
      <line>
        <textedit name="titletext" enabled="false" hiderequiredifdisabled="false" />
        <button name="browsebutton" action="browse" icon="tollium:actions/edit" title="" />
        <button name="clearbutton" action="clear" icon="tollium:actions/delete" title="" />
      </line>
    </contents>
  </fragment>

</screens>
