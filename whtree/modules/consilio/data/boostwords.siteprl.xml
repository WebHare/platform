<siteprofile xmlns="http://www.webhare.net/xmlns/publisher/siteprofile">

  <!-- ADDME: We might want to turn this into a webfeature in the future, but now while we're testing this -->
  <tabsextension xmlns="http://www.webhare.net/xmlns/tollium/screens" name="boostwords" gid="objectprops.boostwords">
    <insert position="settings" where="after">
      <tagedit composition="contentdata" cellname="words" width="1pr" separator=" " />
    </insert>
  </tabsextension>

  <apply>
    <to type="file" />
    <extendproperties extension="#boostwords" contenttype="http://www.webhare.net/xmlns/consilio/boostwords" />
  </apply>

</siteprofile>
