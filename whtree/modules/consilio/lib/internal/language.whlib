// Language texts
PUBLIC RECORD FUNCTION GetConsilioLanguageTexts(STRING langcode)
{
  SWITCH (ToUppercase(langcode))
  {
    CASE "NL"
    {
      RETURN [ unavailable := "De zoekmachine is momenteel niet beschikbaar. Onze excuses voor het ongemak."
             , label_results_multipage := "Resultaten %1 - %2 van de %3 resultaten gevonden voor '%0'"
             , label_results_singlepage := "%3 resultaten gevonden voor '%0'"
             , label_results_justone := "1 resultaat gevonden voor '%0'"
