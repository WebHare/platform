             , label_size := "Grösse"
             , label_modified := "Modifiziert"
             , label_type := "Typ"
             , label_pages := "Seite:"
             , field_searchfor := "Suchen nach"
             , button_search := "Suchen"
             , types := [ doc := "Word-Dokument"
                        , html := "HTML-Seite"
                        , pdf := "PDF-Datei"
                        , txt := "Tekstdatei"
                        , xls := "Excel-Dokument"
                        , unknown := "Unbekannt"
                        ]
             ];
    }
    DEFAULT
    {
      RETURN [ unavailable := "The search engine is currently unavailable. We apologize for the inconvenience."
