             , label_notitle := "(Geen titel)"
             , label_size := "Grootte"
             , label_modified := "Gewijzigd"
             , label_type := "Type"
             , label_pages := "Pagina:"
             , field_searchfor := "Zoek naar"
             , button_search := "Zoek"
             , types := [ doc := "Word-document"
                        , html := "HTML-pagina"
                        , pdf := "PDF-bestand"
                        , txt := "Tekstbestand"
                        , xls := "Excel-document"
                        , unknown := "Onbekend"
                        ]
             ];
    }
    CASE "DE"
    {
